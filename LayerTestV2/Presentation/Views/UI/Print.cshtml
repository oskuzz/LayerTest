@model List<DataAccess.Database.Table.Customer>
@{
    ViewData["Title"] = "Print";
}

<h1>Print</h1>

<div>
    <h2>All Customers</h2>
    <table>
        <thead>
            <tr>
                <td>Customer ID</td>
                <td>Username</td>
                <td>Password</td>
            </tr>
        </thead>
        <tbody>
            @{
                foreach (var c in Model)
                {
                    <tr>
                        <td>@c.CustomerID</td>
                        <td>@c.Username</td>
                        <td>@c.Password</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>